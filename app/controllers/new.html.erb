<%= form_for :@images, url: {action: "create"} , html: {class: "form-horizontal"} do |form| %>
    <div class="form-group">
    <%= form.label :Images %>
     <%= form.file_field :images, :multiple => true, class:'form-control' %>
  </div>
    <div class="form-group">
    <%= form.submit "Create New Room", class: 'btn btn-primary' %>
  </div>

 


<% end %>